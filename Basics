#Q3

.model small
.stack 100h

.data
    SUN = 0
    MON = 1
    TUE = 2
    WED = 3
    THU = 4
    FRI = 5
    SAT = 6

    days db SUN, MON, TUE, WED, THU, FRI, SAT

.code
main proc
    mov ax, @data
    mov ds, ax
    
    mov ah, 4Ch
    int 21h
main endp
end main

######################
Q4

.model small
.stack 100h

.data
    msgGreeting   db 'Hello, World!', 0Dh, 0Ah, '$'
    msgPrompt     db 'Enter your name: ', 0Dh, 0Ah, '$'
    msgFarewell   db 'Goodbye!', 0Dh, 0Ah, '$'
    msgQuestion   db 'How are you today?', 0Dh, 0Ah, '$'
    msgWarning    db 'Warning: Invalid input!', 0Dh, 0Ah, '$'

.code
main proc
    mov ax, @data
    mov ds, ax

    mov ah, 09h
    lea dx, msgGreeting
    int 21h

    mov ah, 09h
    lea dx, msgPrompt
    int 21h

    mov ah, 09h
    lea dx, msgQuestion
    int 21h

    mov ah, 09h
    lea dx, msgWarning
    int 21h

    mov ah, 09h
    lea dx, msgFarewell
    int 21h

    mov ah, 4Ch
    int 21h
main endp
end main
